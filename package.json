{
  "name": "webpack-prpl-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate that allows to easily build multi-target web apps (ES5, ES6) ready to be served by PRPL Server.",
  "scripts": {
    "gulp": "gulp -f gulp/config.ts --cwd .",
    "start": "webpack-dev-server --config webpack/dev.config.ts --hot --color --compress --overlay --history-api-fallback --https --open",
    "build:static": "webpack --config webpack/prod.config.ts",
    "build": "npm run gulp build",
    "serve": "prpl-server --root build/ --host 0.0.0.0 --https-redirect --bot-proxy https://render-tron.appspot.com/render",
    "lint": "tslint -p .",
    "format": "tslint -p . --fix"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@piuccio/ejs-compiled-loader": "^3.0.0",
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/gulp": "^4.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.11.2",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.3",
    "@types/webpack": "^4.4.13",
    "@types/webpack-merge": "^4.1.3",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "gulp": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "htmllint": "^0.7.2",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.0.7",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.0.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.3.1",
    "terser": "^3.9.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "workbox-webpack-plugin": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BoilThatPlate/webpack-prpl-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "prpl-server",
    "boilerplate",
    "starter",
    "typescript",
    "sass",
    "scss",
    "ejs"
  ],
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/BoilThatPlate/webpack-prpl-boilerplate/issues"
  },
  "homepage": "https://github.com/BoilThatPlate/webpack-prpl-boilerplate#readme"
}
